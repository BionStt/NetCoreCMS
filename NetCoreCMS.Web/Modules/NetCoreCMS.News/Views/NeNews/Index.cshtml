@using NetCoreCMS.Modules.News.Models
@using System.Collections

@model List<NeNews>

@{
    Layout = Constants.SiteLayoutName;
    ViewData["PageTitle"] = "News";
    ViewData["PageSubtitle"] = "";
}
    
<div class="row">
    @foreach (var item in Model)
    {
        <div class="col-lg-12">
            <p>
                <a href="/NeNews/Details?newsId=@item.Id" >@item.Name</a>
                <hr />
            </p>
        </div>
    }
</div>

@section Scripts{
    
}
